    "#Functions\n",
    "#write once execute many times\n",
    "def fact(n):\n",
    "    fact=1\n",
    "    for i in range(1,n+1):\n",
    "        fact = fact*i\n",
    "    return fact \n",
    "n= int(input())\n",
    "print(fact(n))"
